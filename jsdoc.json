{
    "source": {
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "include": [
            "src"
        ],
        "exclude": [
            "node_modules",
            "dist",
            "docs"
        ]
    },
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "encoding": "utf8",
        "private": true,
        "readme": "./.github/readme.md",
        "verbose": true
    }
}